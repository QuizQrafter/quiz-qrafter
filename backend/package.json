{
    "name": "backend",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node --env-file-if-exists=.env dist/index.js",
        "dev": "node --import=tsx --env-file-if-exists=.env src/index.ts",
        "check-types": "tsc --noEmit",
        "prisma:deploy": "prisma migrate deploy",
        "predev": "prisma migrate deploy && prisma generate"
    },
    "dependencies": {
        "@google-cloud/documentai": "^8.2.0",
        "@google-cloud/storage": "^7.8.0",
        "@prisma/client": "^5.10.1",
        "bcrypt": "^5.1.1",
        "connect-redis": "^7.1.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-session": "^1.18.0",
        "md-to-pdf": "^5.2.4",
        "multer": "^1.4.5-lts.1",
        "openai": "^4.32.1",
        "prisma": "^5.10.1",
        "redis": "^4.6.13"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.19",
        "tsx": "4.20.6",
        "typescript": "^5.3.3"
    }
}
